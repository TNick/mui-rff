{
  "name": "mui-rff",
  "version": "5.0.0",
  "license": "MIT",
  "description": "Set of modern wrapper components to facilitate using Material UI with React Final Form",
  "repository": {
    "url": "https://github.com/lookfirst/mui-rff.git",
    "type": "git"
  },
  "homepage": "https://lookfirst.github.io/mui-rff",
  "keywords": [
    "react",
    "material-ui",
    "final-form",
    "react-final-form",
    "yup"
  ],
  "author": "Jon Stevens",
  "main": "dist/index.js",
  "module": "dist/mui-rff.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch --rollupTypes",
    "build": "dts build --rollupTypes",
    "test": "dts test",
    "testw": "dts test --watch",
    "lint": "dts lint src test --ignore-pattern node_modules",
    "lint-fix": "yarn lint --fix",
    "ui": "yarn upgradeInteractive; cd example; yarn ui; yarn upgrade",
    "predeploy": "cd example; yarn; yarn build",
    "deploy": "gh-pages -d example/dist",
    "prepublish": "yarn build",
    "postpublish": "yarn deploy",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.0.1",
    "final-form": ">=4.19.1 < 5",
    "react": ">=16",
    "react-final-form": "^6.5.1"
  },
  "optionalDependencies": {
    "@date-io/core": "^2.11.0",
    "@date-io/date-fns": "^2.11.0",
    "@mui/lab": "^5.0.0-alpha.51",
    "date-fns": "^2.25.0",
    "yup": "*"
  },
  "devDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.3",
    "@mui/styles": "^5.0.1",
    "@testing-library/react": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "dts-cli": "*",
    "eslint-config-prettier": "*",
    "eslint-config-react-app": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "eslint-plugin-sort-imports-es6-autofix": "*",
    "eslint-plugin-unused-imports": "*",
    "final-form": "*",
    "gh-pages": "*",
    "husky": "*",
    "jest-environment-jsdom": "*",
    "prettier": "*",
    "react": "*",
    "react-dom": "*",
    "react-final-form": "*",
    "tslib": "*",
    "typescript": "*"
  }
}
